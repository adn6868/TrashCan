class Solution:
    def findSubstring(self, s, words):
        """
        :type s: str
        :type words: List[str]
        :rtype: List[int]
        """
        L = len(words)
        l = len(words[0])
        ans = []
        for i in range(len(s)):
            if valid(s[i:i+L*l],words):
                ans.append(i)
        return ans

def valid(window,words):
    L = len(words)
    l = len(words[0])
    if len(window) != l*L:
        return False
    for i in range(0,L*l,l):
        cur = window[i:i+l]
        if cur in words:
            words.remove(cur)
        else:
            return False
    return len(words) == 0